# Backend API origin used by frontend HTTP requests.
VITE_API_BASE_URL=http://localhost:5000

# Socket.IO origin. If omitted in runtime, frontend falls back to VITE_API_BASE_URL,
# then to http://localhost:5000.
VITE_SOCKET_URL=http://localhost:5000

# Optional log level: debug | info | warn | error
VITE_LOG_LEVEL=debug
